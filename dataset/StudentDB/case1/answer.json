"Course": [
  {"ID": "CS101", "Name": "计算机导论", "Course_Description": "计算机导论课程。本课程旨在为学生构建计算机科学的认知框架，涵盖计算思维、硬件基础及软件原理。", "Credit": 3},
  {"ID": "CS202", "Name": "数据结构", "Course_Description": "数据结构课程。深入剖析数据的逻辑结构（线性表、树、图）与物理存储，重点训练算法设计与复杂度分析能力。", "Credit": 4},
  {"ID": "MKT201", "Name": "市场营销原理", "Course_Description": "市场营销原理课程。探讨市场分析、营销策略组合及消费者行为模式。", "Credit": 3},
  {"ID": "ECO101", "Name": "微观经济学", "Course_Description": "微观经济学课程。研究个体经济单位（消费者、厂商）的决策行为及市场机制。", "Credit": 3},
  {"ID": "ACC305", "Name": "高级会计", "Course_Description": "高级会计课程。针对复杂商业环境下的财务报告、合并报表及特殊会计准则进行深入研讨。", "Credit": 4},
  {"ID": "MGT401", "Name": "战略管理", "Course_Description": "战略管理课程。[高阶研讨] 聚焦企业长期发展规划、竞争优势构建及战略执行。", "Credit": 3},
  {"ID": "MAT101", "Name": "高等数学", "Course_Description": "高等数学课程。主要讲授极限、微积分学及其应用，为后续自然科学与工程学科的学习提供数学工具。", "Credit": 4},
  {"ID": "ENG101", "Name": "大学英语", "Course_Description": "大学英语课程，旨在通过综合训练提升学生的英语语言应用能力与跨文化交际能力。", "Credit": 2},
  {"ID": "PHY101", "Name": "普通物理", "Course_Description": "普通物理课程。系统讲授力、热、电、光、原等物理学基础理论，强调物理模型的建立与分析。", "Credit": 4},
  {"ID": "ART101", "Name": "艺术导论", "Course_Description": "艺术导论课程，培养艺术审美能力，解析不同艺术流派与表现形式。", "Credit": 3},
  {"ID": "HIS101", "Name": "世界历史", "Course_Description": "世界历史课程，概述人类文明的发展历程与重大历史事件。", "Credit": 3},
  {"ID": "PHI101", "Name": "哲学概论", "Course_Description": "哲学概论课程，引导学生思考存在、知识、价值等根本性哲学问题。", "Credit": 3},
  {"ID": "PSY101", "Name": "心理学基础", "Course_Description": "心理学基础课程，探索人类心理活动与行为模式的科学规律。", "Credit": 3},
  {"ID": "BIO101", "Name": "普通生物学", "Course_Description": "普通生物学课程。探索生命现象的基本规律，包括细胞结构、遗传变异及生态系统。", "Credit": 3},
  {"ID": "CHE101", "Name": "基础化学", "Course_Description": "基础化学课程。涵盖无机化学与分析化学基础，探讨物质的组成、结构、性质及变化规律。", "Credit": 4},
  {"ID": "SOC101", "Name": "社会学导论", "Course_Description": "社会学导论课程，分析社会结构、社会关系及社会变迁的基本理论。", "Credit": 3},
  {"ID": "POL101", "Name": "政治学原理", "Course_Description": "政治学原理课程，研究政治权力、国家制度及政治行为的运行逻辑。", "Credit": 3},
  {"ID": "MUS101", "Name": "音乐鉴赏", "Course_Description": "音乐鉴赏课程，通过名作赏析提升音乐感知力与文化素养。", "Credit": 2},
  {"ID": "GEO101", "Name": "自然地理", "Course_Description": "自然地理课程。研究地球表层自然环境的组成、结构、分布规律及其演变过程。", "Credit": 3},
  {"ID": "STA101", "Name": "统计学基础", "Course_Description": "统计学基础课程。涵盖描述统计、概率论基础及推断统计，培养数据分析与量化思维能力。", "Credit": 4}
